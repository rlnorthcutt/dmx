{#
/**
 * @file
 *
 * Default theme implementation for project form.
 *
 * Available variables:
 * - form: The project task form.
 *
 * @ingroup themeable
 */
#}
{{ form.form_build_id }}
{{ form.form_token }}
{{ form.form_id }}

<div class="card">
    <div class="card-content">
        <span class="card-title">Project</span>
        <div class="row">
            <div class="col s12 m6">
                {{ form|without('actions', 'description', 'date') }}
            </div>
            <div class="col s12 m6">
                {{ form.description }}
                {{ form.date }}
            </div>
        </div>
        <div class="card-action">
            {{ form.actions }}
        </div>
    </div>
</div>
